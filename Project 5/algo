bfs(graph g, node v)
{
	unvisit all nodes
	push v onto queue
	visit v
	while(queue is not empty)
	{
		v = front of queue
		for(each unvisited neighbor w of v)
		{
			visit w
			push w onto queue
		}
		pop v off of queue
	}
}

shortestPath(goal g, start s)
{
	if g == s
		return
	else
		shortestPath(goal g, s.pred)
		print predecesor
}